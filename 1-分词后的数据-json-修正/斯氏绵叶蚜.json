{
  "summary": {
    "斯氏绵叶蚜（SinishivaphisszelegiewicziQuednau,1979）是斑蚜科，绵叶蚜属的一种动物。": {}
  },
  "main_body": {
    "形态特征": {
      "描述": {
        "翅孤雌蚜：体椭圆形，活体浅绿色，被白粉及蜡丝": {},
        "玻片标本：头部、胸部深色，腹部淡色，无斑纹，背中蜡片淡色，腹部背片Ⅰ、Ⅱ蜡片边缘深色明显，各蜡片由多环形蜡孔组成": {},
        "触角：节Ⅰ、Ⅱ深色，Ⅱ-Ⅵ各端部黑色": {},
        "喙：淡色，顶端黑褐色": {},
        "前中足：股节、胫节淡色": {},
        "中足：股节端部外缘有黑斑": {},
        "后足：股节端部及胫节基部黑褐色": {},
        "跗节：黑褐色": {},
        "腹管、尾片、尾板及生殖板：淡色": {},
        "气门：圆关闭，气门片淡色": {},
        "体背毛：长尖锐，腹部腹面多毛": {},
        "中额：瘤不隆，额瘤微隆外倾": {},
        "触角：节Ⅰ-Ⅲ基部光滑，其余有小刺突组成横瓦纹": {},
        "触角：毛短尖锐，节Ⅲ有桔瓣形有短睫次生感觉圈，分布于中部": {},
        "喙：短小不达中足基节，节Ⅳ+Ⅴ短楔状，有原生毛和次生毛": {},
        "前、中足股节、胫节：有微刺突横瓦纹，各分散有蜡孔": {},
        "后足股节、胫节：蜡孔密满全节，跗节有刺突横纹": {},
        "前翅脉：粗黑，有昙，翅痣呈U形黑昙，缺径分脉，中脉分三支，镶黑边，两肘脉镶边及宽昙": {},
        "腹管：截断状，光滑，无缘突": {},
        "尾片：瘤状，小刺突分布，有粗长毛": {},
        "尾板：分裂呈两片，有长短毛": {},
        "板：有长毛": {}
      }
    },
    "分布范围": {
      "国内分布：江苏、浙江": {},
      "国外分布：韩国": {}
    }
  }
}