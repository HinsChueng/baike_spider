  "main_body": {
    "形态特征": {
      "观察标本": {
        "标本：5♂♂，2♀♀，云南永胜生角乡，1981．8，22": {},
        "标本：4♂♂7♀♀(3幼)，云南永胜星湖乡，1981．8，22": {}
      },
      "形态描述": {
        "头胸甲：稍隆，分区明显，表面具微细麻点，前鳃区具颗粒突起": {},
        "颈沟：宽浅，明显可辨，胃、心区之间的H形沟较深": {},
        "额：后叶隆起，眼后隆脊较为锋锐": {},
        "额：稍弯向下方，前缘中部内凹": {},
        "背眼缘：埂起，光滑或具微细颗粒，外眼窝角三角形，其侧缘具细锯齿": {},
        "前鳃：齿稍突，前侧缘呈隆脊形，具细锯齿": {},
        "第三颚足：长节的宽度大于长度的1．2—1．3倍，坐节的长度约当宽度的1．4—1．5倍，外肢末端约抵长节基部的1／4—1／5无鞭": {}
      },
      "两螯：不对，腕节背面具微细颗粒及皱襞，内末角具短锐刺，其基部具一小刺": {},
      "掌部：表面及可动指背面基部的1／3具平钝的或明显的突起，大螯掌部的长度大于高度的1．4倍，长于指节，两指内缘具大小不等的三角形齿，合拢时几无空隙": {},
      "步足：粗壮，末对步足前节的长度大于宽度的1．8倍，明显短于指节": {},
      "雄性：腹部三角形，第六节的宽度约当长度的2．2倍，尾节呈圆钝的三角形，宽度大于长度的1．3倍": {},
      "胸部：腹甲沟略浅，胸甲缝间隔沟略窄，第七八节的中纵缝偏短": {},
      "雄性：第一腹肢粗壮，抵达腹锁突，末第二节约当末节长的2．3倍，末节约呈三棱形的锥状，两侧均有较多的长刚毛，第二腹肢沟位于侧面，在末节略转向背方，腹肢孔位于末端": {},
      "第二腹肢：粗壮，末第二节约当末节长的2倍": {},
      "雌性：腹部呈宽大的卵圆形，第六节的宽度约当长度的3倍，尾节的宽度约当长度的2．1倍": {},
      "生殖孔：近圆形，开口斜向上方": {},
      "雄性：头胸甲长18．1—22．3mm，宽22．1—27．1mm": {},
      "雌性：长23．4mm，宽29mm": {}
    },
