{
  "summary": {
    "中华缺鞭米虾：(MancicarisinensisLiang，GuoetTang,1999)是匙指虾科缺鞭米虾属的一种动物": {}
  },
  "main_body": {
    "形态特征": {
      "额角：短而窄，约伸至第1触角柄第1节的末端，上缘具4-8齿，1-2齿位于眼眶后缘的头胸甲上，下缘无齿": {},
      "头胸甲：之前无颊刺": {},
      "第1触角：柄刺超出眼末，为第1触角第1节的0.80倍": {},
      "第2触角：鳞片长为宽的2.8倍": {},
      "第1步足": {
        "描述：短，约伸至第1触角柄第1节的末端，": {},
        "长节：约与腕节等长，腕节末端深凹，长为宽的2.1-2.4倍，": {},
        "掌部：约为指节长的0.63-0.67倍，": {},
        "螯：长为宽的1.9-2.0倍": {}
      },
      "第2步足": {
        "描述：约伸至第1触角第2节的末端，": {},
        "长：节约等于腕节，腕长末缘稍凹陷，长为宽的3.5-4.2倍，": {},
        "指节：为掌部的1.6-1.8倍，": {},
        "螯：长为宽的2.0-2.3倍": {}
      },
      "第3步足：": {
        "描述：超出第1触角柄的末端，": {},
        "长节：约为腕节的2.0倍，腹缘具3刺，": {},
        "腕节：为掌节的0.70倍，": {},
        "掌节：长为宽的7.4-10.3倍，": {},
        "掌节：为指节的3.0-3.3倍，": {},
        "指节：长为宽的4.6-4.8倍，": {},
        "腹缘：具3刺": {}
      },
      "第5步足：": {
        "描述：约伸第1触角柄的末端，": {},
        "长节：为腕节的1.5倍，腹缘具3刺，": {},
        "腕节：为掌节的0.60倍，": {},
        "掌节：长为宽的10.4-13.5倍，为指节长的3.0-4.1倍，": {},
        "指节：长为宽的4.4-4.8倍，": {},
        "腹缘：具18-20个疏状刺": {},
        "雄性：第1腹肢内肢叶片状，基部稍宽，长约为宽的2.8-3.0倍，内缘平直，具近等长的刺": {},
        "外缘：基半部具羽状刚毛，": {},
        "末端：具长的刺，": {},
        "内附肢：由内缘近末端伸出，约1/2超出内肢末端": {}
      }
    },
    "第2腹肢": {
      "雄：附肢呈棒状，内侧和末端具强刺": {},
      "内附肢：发达，伸至雄附肢的1/2处": {},
      "尾节：背面具5-7对活动刺": {},
      "末端：圆": {},
      "中央：不具一小刺突": {},
      "末端：具4-5对刺": {},
      "外侧：刺最粗长": {}
    }
  },
  "生活习性": {
    "分布：于地下水中，从村民的手摇井中采集到，个体小0.98-1.3cm": {},
    "活体：无色透明，眼退化，角膜无色素": {},
    "种群：数量稀少": {}
  },
  "分布范围": {
    "淡水：虾类，仅分布湖南": {}
  },
  "table": {
  }
}