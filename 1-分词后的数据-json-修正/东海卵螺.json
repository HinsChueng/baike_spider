{
  "summary": {
    "东海卵螺：（OocorysdonghaiensisXu,1989）是冠螺科卵螺属的一种动物": {}
  },
  "main_body": {
    "形态特征": {
      "标本：采集地东海，仅有一个标本": {},
      "形态：描述贝壳卵圆形，较小": {},
      "壳质：坚实": {},
      "螺层：约6层，缝合线细": {},
      "胚壳：2层光滑无肋，常被腐蚀": {},
      "螺层：较膨胀，螺旋部圆锥状，体螺层膨圆": {},
      "壳表：具有明显而略宽的螺旋肋，在两肋之间有较宽而微下陷的螺沟，螺旋部每层有螺肋5条，体螺层上则有17条": {},
      "除：螺旋肋外，并具稍弱的纵肋，纵、横螺肋相交处形成小颗粒突起，这种小颗粒突起在螺层的中部和体螺层的上部较明显": {},
      "壳面：黄白色，并被有黄色的壳皮": {},
      "壳口：卵圆形，内乳白色": {},
      "外唇：较薄": {},
      "内唇：滑层较厚，弧形": {},
      "前水管：沟短": {},
      "厣：角质，卵圆形，核位于前端": {},
      "标本：测量(mm)": {},
      "壳：高21．50壳宽15．50": {}
    },
    "分布范围": {
      "国内分布：东海。": {},
      "国外分布：冲绳海槽的东坡，日本宫古岛附近。": {}
    }
