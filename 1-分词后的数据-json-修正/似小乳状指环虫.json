  "main_body": {
    "形态特征": {
      "成虫：形态虫体大型，体长0.67—0.87mm，宽0.17—0.19mm": {},
      "咽：大小0.050—0.063": {},
      "后吸器：与体区分明显，边缘小钩发育完全，全长0.023—0.043mm": {},
      "中央大钩：全长0.090—0.095mm，基部长0.075—0.080mm，内突0.28—0.035mm，外突0.008mm，钩尖长0.033—0.038mm": {},
      "联结：片平直，中部有几处凹凸，大小0.008—0.013×0.038mm": {},
      "交接管：粗壮，呈S形，基部扩宽，管长(依弯曲度)0.048—0.051mm，基径0.005—0.008mm，在呈S状的交接管之前端1/4处管壁变薄，而管径不变细": {},
      "支持器：先为一弧棒，从交接管基部伸出，在近交接管的一侧，有一粗壮几丁质片，向前伸延，扩为山字形，其中两叉粗短，末端尖锐，另一支粗长呈镰刀形": {},
      "交接管：从长支和短支空隙穿过": {},
      "另一侧：有小的乳状突起，支持器长0.030—0.036": {},
      "卵和阴道：未见": {}
    },
    "分布范围": {
      "分布：广东韶关": {}
    }
  },
