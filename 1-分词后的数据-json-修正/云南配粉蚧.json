  "main_body": {
    "形态特征": {
      "雌成虫": {
        "体：多为长纺锤形": {},
        "体长：约3.0mm": {},
        "触角和足：皆很细弱，其各节也较细长": {}
      },
      "触角：7节": {},
      "爪：之下表面无小齿": {},
      "前和后背：裂发达": {},
      "腹裂：3—5个，皆略呈圆形，其大小有各种变化": {},
      "肛环：常具内和外缘2列环孔和6根肛环刺": {},
      "刺孔群：2对，位于腹部末端2节": {},
      "臀瓣：上的刺孔群具2根圆锥形的刺，刺之周围有若干三孔腺和2—3根毛": {},
      "第17对刺孔群：（C17）具有2根圆锥形的刺，但这2根刺较臀瓣上的刺孔群之刺小": {},
      "三孔腺：分布于虫体背和腹面": {},
      "多孔腺和管状腺：在腹部腹面第4—8节腹板上形成横带，在前足基节外侧附近常集聚成小群": {},
      "有些管状腺：还分布在腹部腹面前几节": {},
      "覃状腺：在腹部和胸部形成稀疏的横列，其腹面边缘成纵列": {},
      "体毛：在虫体背面呈刺状，而在腹面则长短不": {}
    },
