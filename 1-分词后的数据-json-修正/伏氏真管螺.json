{
  "summary": {
    "伏氏真管螺：(Euphaedusafitzgeraldae(Boettger,1878))是烟管螺科真管螺属的一种动物": {}
  },
  "main_body": {
    "形态特征": {
      "贝壳：小型，壳质稍厚，坚实，有光泽，呈纺锤形": {},
      "螺层：均膨胀，凸出，前几个螺层增长较快，螺旋部长，呈长塔形": {},
      "壳：面呈角黄褐色或黄褐色，有细致而稠密的螺纹": {},
      "壳：顶尖，缝合线深": {},
      "壳口：呈梨形，口缘厚，外折而扩大，呈白瓷状": {},
      "壳口处：可见上板": {},
      "上板：与下轴板、螺旋板相邻": {},
      "体螺层：右侧壳壁上可见较细长的主襞褶，并与体螺层上的缝合线平行": {},
      "月状襞：弯曲，呈弓形，向下倾斜": {},
      "下腭褶：抵至口缘处": {}
    },
    "生活习性": {
      "喜：阴暗潮湿、多腐殖质的环境": {},
      "生活环境：在灌木丛、草丛中，石块、落叶下，树洞、土石缝隙中，有时也栖息于潮湿多地衣、苔藓的树干、墙壁、岩石上以及多石灰岩地区": {}
    }
