  "main_body": {
    "态特征": {
      "小型：虫体，活体时体长0.300—0.400mm，宽0.070—0.100mm，边缘小钩发育良好，长0.019—0.029mm": {},
      "中央大钩：全长0.030—0.039mm，基部长0.029—0.036mm，内突与钩基部成C形，外突不明显，钩尖0.013—0.015mm": {},
      "联结：片直棒状，中部和两端略宽，大小为0.003—0.004×0.023—0.027mm，副联结片全长0.024—0.035mm，呈人形，每一分支长0.012—0.016mm，两分支末端之距为0.015—0.017mm": {},
      "交接器：全长0.025—0.032mm，交接管呈镰状弯曲，长0.033—0.041mm，管基部呈球形膨大，壁厚，基径0.008—0.012mm，管径0.002—0.003mm": {},
      "支持器：为一弯曲的节片，呈槽状包住交接管，端部形成爪状突": {},
      "卵：椭圆形，具一短趾，卵长0.078mm，宽0.062mm": {},
      "描述：东北、湖北、黑龙江、辽宁、浙江、湖南、广东、四川、广西": {}
    },
    "生活习性": {
      "宿主：棒花鱼Abbottinarivularis(Basilewasky)，蛇鲄SaurogobiodabryiBleeker": {},
      "寄生部位：鳃丝": {},
      "发现地点：兴凯湖": {},
      "感染率：与感染强度检查棒花鱼16尾有4尾阳性，感染强度1—2": {},
      "感染率：8尾蛇鲄有5尾阳性，感染强度1—8": {}
    }
