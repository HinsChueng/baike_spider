{
  "summary": {
    "东方五节根蚜：（FordaorientalisGeorge）是瘿绵蚜科五节根蚜属的一种动物": {}
  },
  "main_body": {
    "形态特征": {
      "无翅孤雌蚜": {
        "体：卵圆形，长2．18，宽1．38mm，活体土黄色": {},
        "玻片标本：头部黑色，前胸两侧缘域黑色，中部淡色，毛基斑明显": {},
        "胸、腹：淡色，腹部背片Ⅶ、Ⅷ有宽横带，横贯全节，胸部中侧毛基斑明显，腹部背面及腹面均有毛基斑，其直径不大于毛长": {},
        "触角、喙、足各节、尾片、尾板及生殖板：黑褐色": {},
        "头部：背片粗糙，前方有粗刺突，中部及后方有粗糙网纹，胸、腹部背面有细刻纹，背片Ⅶ、Ⅷ有瓦纹，腹部腹面细瓦纹": {},
        "气门：圆形，半开放，气门片褐色": {},
        "节间斑：无": {},
        "中胸：腹岔淡色，两臂分开，各横长0．16mm，为触角节Ⅲ的0．92": {},
        "体背：毛短小钝顶，腹部腹面多毛，尖锐，长于背毛，头部有长毛18—20对，前胸中毛3—5对，侧缘毛12—17对，腹部背片Ⅰ—Ⅵ各有中侧毛12—18对，缘毛8—14对，Ⅶ中侧毛5—6对，，缘毛8对，Ⅷ有毛13—20对": {},
        "头顶：毛长0．014mm，为触角节Ⅲ0．41，背片Ⅰ毛长0．009，Ⅷ毛长0．016mm": {},
        "中额：呈圆头状，有背中缝": {},
        "复眼：由3小眼面组成": {},
        "触角：5节，光滑，边缘有皱纹，全长0．50mm，为体长的0．23，节Ⅲ长0．18mm，Ⅰ—Ⅴ长度比例：34，35，100，45，49+18": {},
        "触角：毛钝顶，Ⅰ—Ⅴ毛数：4或5，8—11，16—18，5—10，9—12+4或5根，节Ⅲ毛长0．018mm，为该节直径的0．51": {},
        "原生：感觉圈无睫": {},
        "喙：达后足基节，节Ⅳ十Ⅴ长0．18mm，为该节基宽的2．20倍，为后跗节Ⅱ的1．70倍，有毛8—10对，其中次生短毛5—7对": {},
        "足：各节有皱纹，后股节长0．30mm，为触角节Ⅲ的1．70倍，与节Ⅰ—Ⅲ之和约等长": {},
        "后胫：节长0．46mm，为体长的0．21，毛长0．03mm，为该节直径的0．49": {},
        "后跗节：Ⅱ长0．11mm，跗节Ⅰ各有毛5—7根": {},
        "腹管：无": {},
        "尾片：盔状，有皱纹，长0．07mm，为基宽的0．46，有长毛18—22根": {},
        "尾板：呈椭圆形，有毛40—68根": {},
        "板：椭圆形，有毛40—42根": {},
        "突：3个，各有短毛5—7根": {}
      }
    },
    "生活习性": {
      "寄主": {
        "茅草Imperataarunctinacea(根部)。国外报道取食于高梁和小麦根部": {}
      }
    },
    "分布范围": {
      "国内分布：甘肃(定西2000m，1987-Ⅶ-30，No．8807，张广学、钟铁森)": {},
      "国外分布：印度，巴基斯坦": {}
    }
  }
}