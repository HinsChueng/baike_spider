  "main_body": {
    "形态特征": {
      "体型：细长，褐色，头部光亮，红色，触角、前胸背板侧边、尾铗和足均为褐黄色或浅褐色，尤其足颜色更浅": {},
      "头部：两侧平行，后外缘圆弧形，后缘稍呈弧凹形，额部圆隆，头缝细": {},
      "复眼：小而突出，约为面颊长的1／2": {},
      "触角：细长，基节长大，棒状，第2节短小，长宽几等，第3节细长，第4节短于第3节，第5节和第3节等长，其余各节除端2节外稍细长": {},
      "前胸：背板矩形，后缘弧形，背面前部圆隆，中沟明显，后部散布刻点和皱纹": {},
      "鞘翅：短缩，两侧向后稍扩展，外缘长于缝缘，后缘向内前方斜截，表面平": {},
      "后翅：不发育": {},
      "腹部：狭长，两侧弧形，遍布小刻点，第3-4节背面两侧各有1个小瘤凸": {},
      "末腹：背板短宽，基部较宽，后缘中央微凹，背面前部较高，接近后缘两侧各有1个瘤凸": {},
      "亚末腹：板后缘弧形": {},
      "臀板：小，后缘圆": {},
      "尾铗：后半部弧形，前半部内缘扁扩，边缘直，两支接近": {},
      "足：短壮，后足较长": {},
      "阳茎基：侧突长大，端刺细长，基囊肾形": {},
      "雌虫：末腹背板较狭缩，尾铗较细，向后直伸，无内扩，两支接近，后部不弯曲": {},
      "体": {
        "体长：7：-8．5mm": {},
        "尾铗长：3-3．5mm(♂)，1．7-2mm(♀)": {}
      }
    },
    "分布范围": {
      "国内分布：西藏（吉隆）。": {},
      "国外分布：不丹。": {}
