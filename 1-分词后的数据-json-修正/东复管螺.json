{
  "summary": {
    "东复管螺：（Reiniaeastlakeana(Moellendroff,1882)）是烟管螺科复管螺属的一种动物": {}
  },
  "main_body": {
    "形态特征": {
      "形态：贝壳小型，壳质薄，有光泽，呈胖纺锤形": {},
      "螺层：7个，各螺层均膨大，凸出，前几个螺层增长快，螺旋部高，呈塔形": {},
      "壳：面呈角褐色，并具有横向细致的螺纹和各种白色斑点": {},
      "壳：口很大，呈卵圆形，口缘扩大，外折，向上弯曲": {},
      "上板：边缘垂直，并与螺旋部连接，下轴板掩没": {},
      "褶：体螺层右侧壳壁上可见到主襞褶和腭褶，主襞褶短": {},
      "月状襞：无": {},
      "上腭：褶长": {},
      "下腭：褶短": {},
      "闭板：呈舌形，下部十分扩大，柄细长": {},
      "标本测量": {
        "壳：高11．5mm": {},
        "壳宽：3．25mm": {},
        "壳口：高3．25mm": {},
        "壳口：宽3mm.": {}
      }
    },
    "生长环境": {
      "生活环境：喜欢生活在阴暗潮湿、多腐殖质的环境，如灌木丛、草丛中，石块、落叶下，树洞、土石缝隙中，有时也栖息于潮湿多地衣、苔藓的树干、墙壁、岩石上": {},
      "食物：地衣、苔藓、腐殖质及各种植物幼芽、嫩叶为": {}
    }
  }
}