{
  "summary": {
    "中国内陆溪蟹：（Neilupotamonsinense(TaietSong,1975)）是溪蟹科内陆溪蟹属的一种动物": {},
    "主要：分布在湖南(会同,娄底),广西(资源)": {}
  },
  "main_body": {
    "形态特征": {
      "观察标本": {
        "标本：2♂♂，湖南会同，1973，5": {},
        "标本：3♀♀，湖南会同，1975，11，28": {},
        "标本：2♂♂，湖南娄底，1988，7，20": {},
        "标本：1♂1♀，广西资源，1979，11": {}
      },
      "形态描述": {
        "头胸甲：稍隆，表面具微细凹点，前鳃区具细皱襞": {},
        "颈：沟较宽而凹陷，在中鳃区处环绕成双翅形的前、后隆块，胃、心区之间具H形细沟": {},
        "额：后叶与眼后隆脊相连圆钝": {},
        "额：稍弯向下方，中部稍凹": {},
        "背眼缘：埂起，眼后区较凹，外眼窝角呈低平三角形，其外侧缘具颗粒齿约6枚": {},
        "前鳃齿：圆钝，明显，前侧缘短于后侧缘，具颗粒齿约16—18枚": {},
        "第三颚足：长节的宽度约为长度的1.1倍，坐节的长度约当宽度的1.6倍，外肢末端约抵长节基部的1／3，具短鞭": {}
      },
      "两螫：不对称，腕节表面具凹点，内末角具短锥状刺，其基部具一隆起，表面具颗粒突起4—5枚": {},
      "大螯：掌部背、外侧面具凹点，内侧面的下半部具细颗粒，长度约当高度的1.3倍，约当可动指长的1.1倍": {},
      "两指：内缘具平钝齿，合并时略具空隙": {},
      "步足：细长、光滑，末对步足前节的长度约为宽度的2.1倍，短于指节": {},
      "雄性": {
        "腹部呈塔状三角形，第六节的宽度约当长度的2.2倍，尾节三角形，宽度大于长度的1.4倍": {},
        "胸部：腹甲缝的中沟略窄，第七八节的中纵缝较短": {},
        "第一腹肢：末端超过腹锁突，末第二节约当末节长的1.9倍，末节的长度约当宽度的3.8倍，腹叶基部略突，呈平钝的圆叶状，末端腹肢呈瓶口状": {},
        "第二腹肢：末第二节约当末节长的1.9倍": {},
        "头胸甲：长17.5—18.6mm，宽22.3—24mm": {}
      },
      "雌性": {
        "腹部：长圆形，第六节的宽度约当长度的2.9倍，尾节的宽度约为长度的2.1倍，生殖孔呈纵长的圆方形，较小，仅占第六胸甲内侧上角的上半部，开口朝向内方": {},
        "头胸甲：长18.6mm，宽24.1mm": {}
      }
    },
    "生活习性": {
      "栖息：于溪流或灌渠岸边洞穴中，并在溪中取食活动": {}
    },
    "主要分布": {
      "国内分布：湖南(会同、娄底)，广西(资源)。": {}
    }
  },
