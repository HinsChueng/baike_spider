  "main_body": {
    "形态特征": {
      "体型：细长，黑褐色或褐色，鞘翅褐黄色，尾铗赤褐色或褐色": {},
      "头部：较宽，两颊弧圆形，额部圆隆，头缝明显": {},
      "复眼：突出": {},
      "触角：细长，12节，基节长大，第2节长宽几等，第3节和第4节几等长，第5节稍长于第4节，其于各节较细长": {},
      "前胸：背板宽稍大于长，接近方形，后缘弧圆形，表面前部稍圆隆，后部平": {},
      "前后：翅发达，鞘翅狭长，两侧平行，后缘几呈截形，表面平滑": {},
      "后翅：翅柄长稍短于前翅长的l／2，后缘稍圆": {},
      "腹部：长大，两侧弧形，遍布小刻点": {},
      "末腹：背板横宽，两侧向后强度收缩，后缘中部稍呈弧凹形，表面拱形，稍光亮": {},
      "臀板：短小，后缘波曲形": {},
      "雄虫：尾铗甚细长，向后几乎直伸，基部扁粗，顶端尖，向内侧弯，内缘中部前有1尖刺突": {},
      "雌虫：两尾铗向后直伸，相互接近，内缘无刺突": {},
      "足：细长": {},
      "阳茎基：侧突细长，顶端钝圆形，阳茎端刺细长，基囊弯曲": {},
      "体": {
        "长：8：-1lmm": {},
        "尾铗：长：8-9mm(♂)，5-6mm(♀)": {}
      }
    },
    "分布范围": {
      "国内分布：陕西（留坝），云南": {},
      "国外分布：朝鲜，日本": {}
    }
  }
