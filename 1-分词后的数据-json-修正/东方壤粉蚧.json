{
  "summary": {
    "东方壤粉蚧：(HumococcusorientalisBorchsenius,1949)是粉蚧科壤粉蚧属的一种动物": {}
  },
  "main_body": {
    "形态特征": {
      "雌成虫": {
        "体：椭圆形": {},
        "虫体：长约2.6mm，宽1.6mm": {},
        "触角：7节": {},
        "胸足：小型，分节正常，后足基节很宽有成群透明孔": {},
        "爪：细长，爪之下表面无小齿，爪冠毛长于爪，其顶端略膨大）胸气门周围有三孔腺和多孔腺分布，气管较宽": {},
        "前：背裂缺，后背裂存在，但常小": {},
        "腹裂：无": {},
        "肛环：近圆形，无肛环孔，有6根短细肛环毛": {},
        "臀瓣：不显，其上具长端毛和4根长毛": {},
        "三孔腺：存在于体背面和腹面，在背面均匀分布，但数量较少。在腹面沿体缘和中区均有分布": {},
        "多孔腺：在头胸部的腹面、腹部腹面的中部呈横列分布，在体缘和第4腹节腹板中区成群或分散分布": {},
        "管腺：为蕈状腺，分布于体背面和腹面的边缘和头胸部中区及在腹部各节中区成横列": {},
        "单孔：在背面和腹面分布均较多": {},
        "体毛：在体背面很长，排成横列，每体节在边缘有2—3根长毛，体腹面体毛较稀疏且短": {}
      }
    },
    "生活习性": {
      "寄主：蒿属植物": {}
    },
    "分布范围": {
      "国内分布：内蒙古": {},
      "国外分布：蒙古,朝鲜,俄罗斯,哈萨克斯坦": {}
    }
  }
}