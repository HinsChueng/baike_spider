  "main_body": {
    "形态特征": {
      "体形：狭小，黑褐色或黑色，头部暗红色，触角褐红色，前胸背板两侧灰白色，前翅和后翅翅柄暗褐色，腹部黑色或红褐色": {},
      "头部：较大，两铗圆弧形，后缘横直，额部圆隆，头缝较细": {},
      "复眼：小，黑色": {},
      "触角：12节，基节长而粗，棍棒形，第2节长稍短于宽，第3节长于第4节，第5节长于第3节，其余各节较细长": {},
      "前胸：背板稍短宽，两侧平行，后缘圆弧形，背面前部圆隆，中央有1纵向沟，后部平，散布明显刻点和皱纹": {},
      "鞘翅：发达，肩部圆弧形，两侧平行，后缘接近横直，表面光滑": {},
      "后翅：翅柄突出，后外缘弧圆形": {},
      "腹部：狭长，两侧弧形，遍布小刻点，第3—4节背面两侧各有1瘤凸，后一对较明显": {},
      "末腹：背板短宽，两侧向后收缩，后缘中央弧凹形，表面散布小刻点，接近后缘两侧各有1隆凸": {},
      "亚末腹：板后缘圆弧形，表面散布刻点和皱纹": {},
      "臀板：后缘圆弧形": {},
      "尾铗：弧弯形，基部内缘扁扩部分较短，其后向外强度弯曲，顶端尖": {},
      "足：稍细长，前足较短": {},
      "雄性：外生殖器地阳茎基侧突外缘弧形，端部圆，阳茎端刺细长，基囊向下弯": {},
      "雌虫：形似雄虫，但尾铗较短，不弯曲，基部较宽，顶端尖，向内侧弯，两支内缘接近": {},
      "体": {
        "体长：7：．1-10．8mm": {},
        "尾铗长：2．5-3mm(♂)，1．5～2．5mm(♀)": {}
      }
    },
    "分布范围": {
      "国内分布：云南": {},
      "国外分布：不丹，尼泊尔，缅甸，印度，锡金。": {}
    }
  },
