  "summary": {
    "云南近溪蟹指名亚种：(PotamiscusyunnanenseyunnanenseKemp,1923)是溪蟹科近溪蟹属的一种动物": {}
  },
  "main_body": {
    "形态特征": {
      "观察标本：2♂♂3♀♀，云南大理邓川永安江，1964，7，10。": {},
      "形态描述": {
        "头胸甲：前半部稍隆，表面具微细凹点，前鳃区具细皱襞": {},
        "颈：沟明显，胃、心区之间的H形沟较深": {},
        "额：后叶圆钝，眼后隆脊较隆，眼后区稍凹": {},
        "额弯：向下方，前缘中部稍凹": {},
        "背眼缘：埂起，外眼窝角三角形，指向前方，其外侧缘具不明显的微细锯齿": {},
        "前鳃：齿稍大，前侧缘具不突出的细锯齿18—19枚": {},
        "第三颚足：长节的宽度约为长的1．2倍，长节的长度约当宽度的1．4倍，外肢无鞭，末端约抵长节基部的1／4": {}
      },
      "两螯：不甚对称，腕节背面具鳞状突起，近边缘具颗粒，内末角具一锐刺，基部具一附属小齿": {},
      "大螯：掌节外侧面具鳞状斑及颗粒，长度约当高度的1．4倍，两指粗壮，内缘具大小不等的三角形齿，合拢时几无空隙": {},
      "步足：粗壮，第二对最长，末对步足前节的长度约当宽度的1．7倍，指节约当前节长1．1倍": {},
      "雄性：腹部三角形，第六节基部的宽度约当长度的2．1倍，尾节的宽度约当长度的1．2倍": {},
      "胸部：腹甲沟中等深度，胸甲缝间隔沟稍宽，第七八节的中纵缝稍短": {},
      "第一腹肢：粗短，末端约抵腹锁突，末第二节约当末节长的1．9倍，末节锥形，第二腹肢沟位于侧面，腹肢孔位于末端": {},
      "第二腹肢：粗壮，约与第一腹肢等长，末第二节的长度约当末节长的2．1倍": {},
      "雌性：腹部宽卵形，第六节基部的宽度约当长度的3．1倍，尾节的宽度约当长度的2．5倍，末缘略显平钝，第六七，七八节缝内侧接近中纵缝": {},
      "生殖孔：近圆形，开口朝向上方，两孔稍远离": {},
      "雄性：头胸甲长15．9—18．2mm，宽19．1—22．2mm": {},
      "雌性：长19．2—27．1mm，宽24．1—29mm": {}
    },
    "分布范围": {
      "国内分布：云南：(洱海、临沧、大理)": {}
    }
  },
