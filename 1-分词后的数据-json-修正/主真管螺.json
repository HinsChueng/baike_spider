  "main_body": {
    "形态特征": {
      "贝壳：中等大小，无脐孔，呈纺锤形，有光泽": {},
      "螺层：有12个，各螺层均膨胀": {},
      "螺层：下部有扁肋，其基部为椭圆形，淡白色，螺旋部细长，呈长塔形": {},
      "壳：面呈栗褐色，并有细致而稠密的、柔弱的刻纹和肋状突起": {},
      "壳顶：钝，缝合线深": {},
      "壳口：卵圆形、倾斜": {},
      "口缘：薄，具蛋白色的唇，增厚，外折": {},
      "下板：粗壮，隐蔽，稍弯曲上升": {},
      "下轴板：不明显，上板分离，与螺旋板相连": {},
      "体螺层：右侧壳壁内有腭褶，主襞褶长，月状襞稍长而宽": {},
      "标本测量：": {},
      "壳：高19—20mm，宽4mm": {},
      "壳口：高4—4．3mm，宽3．7—4．2mm": {}
    },
    "生活习性": {
      "生活位置：在灌木丛、草丛中,石块落叶下,树洞石缝中,也在潮湿多地衣和苔藓的树干、墙壁或岩石上,在平原、丘陵、森林、溪河边,公园、住宅、寺庙附近": {}
    },
    "分布范围": {
      "国内分布：湖南岳阳": {}
    }
